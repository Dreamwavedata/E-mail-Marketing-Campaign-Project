WITH CampaignRates AS (
    SELECT 
        Campaigns.campaign_name,
        SUM(total_unsubscribes) AS total_unsubscribes,
        SUM(total_sent) AS total_sent
    FROM 
        Campaign_Performance
    LEFT JOIN 
        Campaigns ON Campaign_Performance.campaign_id = Campaigns.campaign_id
    GROUP BY 
        Campaigns.campaign_name
)

SELECT 
    campaign_name,
    ROUND(total_unsubscribes * 100.0 / NULLIF(total_sent, 0), 2) AS Unsubscribe_rate
FROM 
    CampaignRates
ORDER BY 
    Unsubscribe_rate DESC;

	
-- Q2. Do certain campaign categories(promotion, newsletter, re-engagement, announcement) have higher unsubscribe rates?
SELECT
	Campaigns.category,
 ROUND(SUM(total_unsubscribes)*100.0/ SUM(total_sent),2) AS Unsubscribe_rate
 FROM
	Campaign_Performance
 LEFT JOIN 
	Campaigns ON Campaign_Performance.campaign_id=Campaigns.campaign_id
 GROUP BY 
	category
 ORDER BY
	unsubscribe_rate DESC;

-- Q3. What is the Overall_unsubscribe_rate?
SELECT
	ROUND(SUM(total_unsubscribes)*100.0/ SUM(total_sent),2) AS Unsubscribe_rate
FROM 
	Campaign_Performance;
	
-- Q5. Which region(s) or device type(s) have the highest unsubscribe rate?
SELECT
	device_type, region, 
	COUNT (*) AS total_unsubscribes
FROM	
	Users, Campaign_Performance
GROUP BY
	region, device_type
ORDER BY
	total_unsubscribes DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
